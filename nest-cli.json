{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/rest/src",
  "monorepo": true,
  "root": "apps/rest",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/rest/tsconfig.app.json"
  },
  "projects": {
    "rest": {
      "type": "application",
      "root": "apps/rest",
      "entryFile": "main",
      "sourceRoot": "apps/rest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rest/tsconfig.app.json"
      }
    },
    "queue": {
      "type": "application",
      "root": "apps/queue",
      "entryFile": "main",
      "sourceRoot": "apps/queue/src",
      "compilerOptions": {
        "tsConfigPath": "apps/queue/tsconfig.app.json"
      }
    }
  }
}